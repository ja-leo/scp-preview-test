"token punctuation\">,</span>\n  changeDetection<span class=\"token punctuation\">:</span> ChangeDetectionStrategy<span class=\"token punctuation\">.</span>OnPush\n<span class=\"token punctuation\">}</span><span class=\"token punctuation\">)</span>\n<span class=\"token keyword\">export</span> <span class=\"token keyword\">class</span> <span class=\"token class-name\">NzDemoDaySelectorReactiveComponent</span> <span class=\"token keyword\">implements</span> <span class=\"token class-name\">OnInit</span> <span class=\"token punctuation\">{</span>\n  daysOfWeek<span class=\"token operator\">?</span><span class=\"token punctuation\">:</span> DaysOfWeek<span class=\"token punctuation\">;</span>\n  dayForm<span class=\"token punctuation\">:</span> FormGroup<span class=\"token punctuation\">;</span>\n\n  <span class=\"token keyword\">constructor</span><span class=\"token punctuation\">(</span><span class=\"token keyword\">private</span> readonly fb<span class=\"token punctuation\">:</span> FormBuilder<span class=\"token punctuation\">)</span><span class=\"token punctuation\">{</span><span class=\"token punctuation\">}</span>\n\n  <span class=\"token function\">ngOnInit</span><span class=\"token punctuation\">(</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">:</span> <span class=\"token keyword\">void</span> <span class=\"token punctuation\">{</span>\n    <span class=\"token keyword\">this</span><span class=\"token punctuation\">.</span>daysOfWeek <span class=\"token operator\">=</span> <span class=\"token punctuation\">{</span>\n      monday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span><span class=\"token punctuation\">,</span>\n      tuesday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span><span class=\"token punctuation\">,</span>\n      wednesday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span><span class=\"token punctuation\">,</span>\n      thursday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span><span class=\"token punctuation\">,</span>\n      friday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span><span class=\"token punctuation\">,</span>\n      saturday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span><span class=\"token punctuation\">,</span>\n      sunday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">false</span>\n    <span class=\"token punctuation\">}</span>\n\n    <span class=\"token keyword\">this</span><span class=\"token punctuation\">.</span>dayForm <span class=\"token operator\">=</span> <span class=\"token keyword\">this</span><span class=\"token punctuation\">.</span>fb<span class=\"token punctuation\">.</span><span class=\"token function\">group</span><span class=\"token punctuation\">(</span><span class=\"token punctuation\">{</span>\n      dayFormControl<span class=\"token punctuation\">:</span> <span class=\"token punctuation\">[</span><span class=\"token keyword\">this</span><span class=\"token punctuation\">.</span>daysOfWeek<span class=\"token punctuation\">]</span>\n    <span class=\"token punctuation\">}</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span>\n  <span class=\"token punctuation\">}</span>\n\n  <span class=\"token function\">asIsOrder</span><span class=\"token punctuation\">(</span>_a<span class=\"token punctuation\">:</span> <span class=\"token keyword\">any</span><span class=\"token punctuation\">,</span> _b<span class=\"token punctuation\">:</span> <span class=\"token keyword\">any</span><span class=\"token punctuation\">)</span> <span class=\"token punctuation\">{</span>\n    <span class=\"token keyword\">return</span> <span class=\"token number\">1</span><span class=\"token punctuation\">;</span>\n  <span class=\"token punctuation\">}</span>\n<span class=\"token punctuation\">}</span>\n\n<span class=\"token keyword\">export</span> <span class=\"token keyword\">class</span> <span class=\"token class-name\">DaysOfWeek</span> <span class=\"token punctuation\">{</span>\n  monday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n  tuesday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n  wednesday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n  thursday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n  friday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n  saturday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n  sunday<span class=\"token punctuation\">:</span> <span class=\"token keyword\">boolean</span><span class=\"token punctuation\">;</span>\n<span class=\"token punctuation\">}</span>\n","rawCode":"import { Component, ChangeDetectionStrategy, OnInit } from '@angular/core';\nimport { FormBuilder, FormGroup } from '@angular/forms';\n\n@Component({\n  selector: 'nz-demo-day-selector-reactive',\n  template: `<h3>Day selector demo using Reactive Form</h3>\n            <form [formGroup]=\"dayForm\">\n                <p>\n                    <sc-day-selector formControlName=\"dayFormControl\"></sc-day-selector>\n                </p>\n                <div *ngFor=\"let day of daysOfWeek | keyvalue : asIsOrder\">\n                  <label>The Selected day {{ day.key }} is: {{ day.value }}</label>\n                </div>\n            </form>`,\n  changeDetection: ChangeDetectionStrategy.OnPush\n})\nexport class NzDemoDaySelectorReactiveComponent implements OnInit {\n  daysOfWeek?: DaysOfWeek;\n  dayForm: FormGroup;\n\n  constructor(private readonly fb: FormBuilder){}\n\n  ngOnInit(): void {\n    this.daysOfWeek = {\n      monday: false,\n      tuesday: false,\n      wednesday: false,\n      thursday: false,\n      friday: false,\n      saturday: false,\n      sunday: false\n    }\n\n    this.dayForm = this.fb.group({\n      dayFormControl: [this.daysOfWeek]\n    });\n  }\n\n  asIsOrder(_a: any, _b: any) {\n    return 1;\n  }\n}\n\nexport class DaysOfWeek {\n  monday: boolean;\n  tuesday: boolean;\n  wednesday: boolean;\n  thursday: boolean;\n  friday: boolean;\n  saturday: boolean;\n  sunday: boolean;\n}\n"}
