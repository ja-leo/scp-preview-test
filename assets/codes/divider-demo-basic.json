"import {Component} from '@angular/core';\n\n@Component({\n  selector: 'nz-demo-divider-basic',\n  template: `\n    <div class=\"demo-divider\">\n      <h3>Divider Component</h3>\n      <br>\n      <br>\n      <div>\n        <h4>\n          nzType: horizontal, nzOrientation: left, nzText: Put your text here\n          <nz-divider nzText=\"Put your text here\" nzOrientation=\"left\" nzType=\"horizontal\"></nz-divider>\n        </h4>\n      </div>\n      <br>\n      <br>\n      <div>\n        <h4>\n          nzType: horizontal, nzOrientation: center, nzDashed: true, nzText: Put your text here\n          <nz-divider nzText=\"Put your text here\" nzOrientation=\"center\" nzType=\"horizontal\" [nzDashed]=\"true\"></nz-divider>\n        </h4>\n      </div>\n      <br>\n      <br>\n      <div>\n        <h4>\n          nzType: horizontal, nzOrientation: right, nzPlain: true, nzText: Put your text here\n          <nz-divider nzText=\"Put your text here\" nzOrientation=\"right\" nzType=\"horizontal\" [nzPlain]=\"true\"></nz-divider>\n        </h4>\n      </div>\n      <br>\n      <br>\n      <div>\n        <h4>\n          No parameters past\n          <nz-divider></nz-divider>\n        </h4>\n      </div>\n      <br>\n      <br>\n      <div>\n        <h4>\n          nzType\n          <nz-divider nzType=\"vertical\"></nz-divider>\n          vertical\n        </h4>\n      </div>\n      <br>\n      <br>\n      <div>\n        <h4>\n          nzType\n          <nz-divider nzType=\"vertical\" [nzDashed]=\"true\"></nz-divider>\n          vertical\n          <nz-divider nzType=\"vertical\" [nzDashed]=\"true\"></nz-divider>\n          nzDashed\n          <nz-divider nzType=\"vertical\" [nzDashed]=\"true\"></nz-divider>\n          true\n        </h4>\n      </div>\n      <br>\n      <br>\n      <div style=\"width: 777px;\">\n        <h4>\n          nzText: Custom Template, nzOrientation: right\n          <nz-divider [nzText]=\"iconText\" nzOrientation=\"right\"></nz-divider>\n          <ng-template #iconText>\n            <nz-avatar [nzSize]=\"64\" nzIcon=\"ja:jobadderlogo\"></nz-avatar>\n            <nz-divider nzType=\"vertical\"></nz-divider>\n            Avatar + Text + button\n            <nz-divider nzType=\"vertical\"></nz-divider>\n            <button nz-button>click me</button>\n          </ng-template>\n        </h4>\n      </div>\n    </div>\n  `\n})\nexport class NzDemoDividerBasicComponent {\n}\n"}
